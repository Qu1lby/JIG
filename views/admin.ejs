	<section id="fh5co-home" data-section="Evenements" style="background-image: url(images/full_image_2.jpg);" data-stellar-background-ratio="0.65">
		<div class="gradient"></div>
		<div class="container">
			<div class="text-wrap" style="height:750px">
				<div class="text-inner">
					<div class="row">
						<div class="col-md-8 col-md-offset-2">
							<h2 class="to-animate" style="color: rgba(255, 255, 255, 1)">Ajouter/Supprimer un événement et les Points/Paris associés</h2>
							<div class="row" style="margin-top:50px">
								<div class="col-md-8 col-md-offset-2" style="color:black">
									<input id="event_input" maxlength="300" style="width:70%;background-color:rgba(240, 240, 240, 0.5)" type="text">
									<button style="background-color:rgba(240, 240, 240, 0.5)" id="add_event"><i class="icon icon-plus"></i></button>
									<button style="background-color:rgba(240, 240, 240, 0.5)" onClick="window.location.reload()"><i class="icon icon-refresh"></i></button>
								</div>
							</div>
							<div  class="table-responsive" style="margin-top:20px; height:250px; overflow:auto; border: 1px groove rgba(146, 146, 146, 0.25)">
								<table>
									<% for (var i = 0; i < arg['events'].length; i++){ %>
										<tr>
											<td style="color:black;"><%= arg['events'][i].libelle %></td>
											<td style="color:black;">
												<a class="trash_event" href="#" id_event="<%= arg['events'][i].id %>">
													<i class="icon to-animate icon-trash" style="color:black"></i>
