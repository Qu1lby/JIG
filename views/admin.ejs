	<script src="js/main.js"></script>
	<script src="js/socket.io-1.3.5.js"></script>
	<script src="js/notify.min.js"></script>

	<script>
	  var socket = io();

		$(".trash_event").on('click', function() {
			socket.emit('del_event', {
				id_event : $(this).attr('id_event')
			});
		});

		$("#add_event").on('click', function() {
			socket.emit('add_event', {
				lib_event : document.getElementById("event_input").value
			});
		});

		$("#reset_first").on('click', function() {
			document.getElementById("reset_all").style.display = 'initial'
		});

		$("#reset_all").on('click', function() {
			document.getElementById("reset_all_final").style.display = 'initial'
		});

		$("#reset_all_final").on('click', function() {
			socket.emit('reset_data');
		});

		$.notify.addStyle('happyblue', {
		  html: "<div><span data-notify-text/></div>",
		  classes: {
		    base: {
		      "white-space": "nowrap",
		      "background-color": "white",
		      "padding": "5px",
					"border": "1px groove"
		    }
		  }
		});

		socket.on('end', function(data){
			$.notify(data.msg,{position:"bottom right", style:"happyblue"});
		});
	</script>

